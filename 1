#include <iostream>
#include <string>
#include <algorithm> 

double calculateMatchPercentage(const std::string &correctText, const std::string &textToCompare) {
    if (correctText.empty() && textToCompare.empty()) {
        return 100.0; 
    }

    size_t matchCount = 0;
    size_t length = std::min(correctText.length(), textToCompare.length());


    for (size_t i = 0; i < length; ++i) {
        if (correctText[i] == textToCompare[i]) {
            ++matchCount;
        }
    }

    double matchPercentage = (static_cast<double>(matchCount) / correctText.length()) * 100.0;

    return matchPercentage;
}

int main() {
    std::string correctText;
    std::string textToCompare;

    std::getline(std::cin, correctText);
    std::getline(std::cin, textToCompare);


    bool areEqual = (correctText == textToCompare);
    std::cout << "Тексты совпадают: " << std::boolalpha << areEqual << std::endl;

    double matchPercentage = calculateMatchPercentage(correctText, textToCompare);
    std::cout << "Процент совпадения текстов: " << matchPercentage << "%" << std::endl;

    return 0;
}
